<?xml version="1.0" encoding="utf-8"?>
<s:NavigatorContent xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" 
					xmlns:mx="library://ns.adobe.com/flex/mx"
					xmlns:tf="com.geae.component.time.*">
	<s:layout>
		<s:VerticalLayout/>
	</s:layout>
	
	<s:HGroup paddingLeft="20" gap="10" paddingBottom="10" paddingRight="10" paddingTop="10">
			<tf:TimeField id="hdebut" label="Heure début"/>				
			<tf:TimeField id="hfin" label="Heure fin"/>
			<mx:Spacer width="30"/>
			<mx:Image source="{Icons.ADD}" toolTip="Ajouter"/>
	</s:HGroup>

	<s:HGroup width="100%" horizontalAlign="center">		
		<s:BorderContainer height="1" width="700" borderColor="0xCCCCCC"/>
	</s:HGroup>
		
	
	<fx:Declarations>
		<!-- Placer ici les éléments non visuels (services et objets de valeur, par exemple). -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			import com.geae.asset.Icons;
			[Bindable]
			public var etape:String;
		]]>
	</fx:Script>
</s:NavigatorContent>
